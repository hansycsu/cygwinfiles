alias ls='ls -F --color=auto --group-directories-first --time-style=long-iso'
alias ll='ls -l'
alias lla='ls -lA'
alias la='ls -A'
# A backslash before command to avoid recursion and define
alias l='\ls'
alias dirs='dirs -v'
alias so='source'
alias grep='grep --color=auto'
